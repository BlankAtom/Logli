{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "Authentication": {
        "Authority": "https://sample.auth0.com",
        "ResponseType": "code",
        "ClientId": "USER_SECRET",
        "ClientSecret": "USER_SECRET",
        "Scope": [
            "openid",
            "profile",
            "email"
        ],
        "CallbackPath": "/signin-auth0",
        "ClaimsIssuer": "Auth0"
    }
}